{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "babel calOG.js --out-file ./csvFiles/cal.cjs",
    "packageWindows": "pkg ./csvFiles/cal.cjs --target node16-win-x64 --output ./csvFiles/cal.exe",
    "packageLinux": "pkg ./csvFiles/cal.cjs --target node16-linux-x64 --output ./csvFiles/cal",
    "buildTri": "babel ./routes/triSearch.js --out-file ./routes/triS.cjs",
    "packageTri": "pkg ./routes/triS.cjs --target node16-linux-x64 --output ./routes/triS"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "nodemailer": "^7.0.3",
    "pkg": "^5.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "nodemon": "^3.1.7"
  }
}
